{
  "problemMatcher": [{
    "owner": "tap",
    "pattern": [{
      "regexp": "^\\s*not ok (.+)$",
      "message": 1
    }, {
      "regexp": "^\\s*---"
    }, {
      "regexp": "^\\s*compare"
    }, {
      "regexp": "^\\s*at:"
    }, {
      "regexp": "^\\s*line: (\\d+)",
      "line": 1
    }, {
      "regexp": "^\\s*column: (\\d+)",
      "column": 1
    }, {
      "regexp": "^\\s*file: (.*)",
      "file": 1
    }]
  }]
}
